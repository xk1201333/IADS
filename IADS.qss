QWidget {
    background-color: #FFFFFF;
}

#ImglistWidget{
    border:0px
}



/*菜单*/
QMenu{
    background:white;
    border:1px solid lightgray;
}
QMenu::item{
    padding:5px 15px;
    margin:1px 2px;
    color:#222222;
}

QMenu::item:hover{
    background:#DAEBF7;
}

QMenu::item:selected{
    background:#DAEBF7;
}

/*下拉框*/
QComboBox {
    color:#4A7298;
    border: 2px dashed rgb(203, 224, 178);
    border-radius: 3px;
    padding: 1px 18px 1px 3px;
    font: 10pt bold "Times New Roman";
}


/* 编辑框*/
QLineEdit {
    background:transparent;
    border: 1px solid lightgray;
    font: bold "Times New Roman";
}


/*ToolBox*/
QToolBox:tab {
    font-size: 20px;
    font-family: "Times New Roman";
    background-color: #F0FFFF;
}

QToolBox:tab:hover {
    font-size: 23px;
    font-family: "Times New Roman";
    background-color: qlineargradient(spread:pad, x1:0,x2:1,stop:0 #159957, stop:1 #155799);
}

QToolBox:tab:selected{
    font-size: 23px;
    font-family: "Times New Roman";
    font-weight: bold;
    background-color:#008080;
}

QGraphicsView{
  background: transparent;
  border:0px
}




QTreeView {
  border:0px
}
QTabWidget::pane {
  border: 1px dashed #55AAC6;
}


/* 标题设置 */
QDockWidget::title {
  background-color: transparent;/* 设置标题栏背景颜色 */
}
QDockWidget::title:hover{
  background-color: #9C93E6;
}


/* 关闭和显示按钮 */
QDockWidget::close-button, QDockWidget::float-button {

}

/* 鼠标悬浮状态 */
QDockWidget::close-button:hover, QDockWidget::float-button:hover {

}

/* 鼠标按下状态 */
QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {

}



/*按键*/
